<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jxd.orderingsys.dao.IEmpMapper">


    <select id="selectByDeptId" resultType="com.jxd.orderingsys.model.Emp">
        select id,name,sex,phone,dept_id,role from emp
        where dept_id = #{deptId}
    </select>
</mapper>